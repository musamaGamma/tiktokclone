{
  "expo": {
    "name": "tikTokClone",
    "slug": "tikTokClone",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/tiktok.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/tiktok.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff",
      "dominantColor": "#ffffff",
      "width": 100,
      "height": 100
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow tiktok to access your camera."
        }
      ]
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/tiktok.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.mosama00.tikTokClone"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "f351c641-43bb-4d9f-b267-4a5f2105eeef"
      }
    }
  }
}
